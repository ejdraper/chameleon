<?xml version="1.0" encoding="UTF-8"?>
<root>
  <% @data[:items].each do |item| %>
    <item><%= item %></item>
  <% end %>
  <settings>
    <% @data[:x_axis].each do |x| %>
      <axisx><%= x %></axisx>
    <% end %>
    <% @data[:y_axis].each do |y| %>
      <axisy><%= y %></axisy>
    <% end %>
    <colour><%= @data[:colour] || "ff9900" %></colour>
  </settings>
</root>
